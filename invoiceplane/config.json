{
  "name": "Nginx web server",
  "version": "1.0.0",
  "slug": "nginx",
  "description": "Web server powered by Nginx",
  "startup": "services",
  "boot": "auto",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    {
      "name": "config",
      "type": "config",
      "source": "config"
    },
    {
      "name": "ssl",
      "type": "config",
      "source": "ssl"
    },
    {
      "name": "webroot",
      "type": "config",
      "source": "webroot"
    }
  ],
  "homeassistant": {},
  "services": [
    {
      "name": "Nginx web server",
      "description": "Starts the Nginx web server",
      "startup": true,
      "schema": {}
    }
  ]
}
